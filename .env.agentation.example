# Copy to .env.local for local development

# Agentation sends submit events here from the in-app dev panel
VITE_AGENTATION_WEBHOOK_URL=http://localhost:8787

# Webhook automation listener defaults
PORT=8787
TRIGGER_EVENTS=submit
AGENTATION_MODE=autopilot

# Optional: enable implementation and review automation commands
# AGENTATION_IMPLEMENT_COMMAND=pnpm typecheck
# AGENTATION_REVIEW_COMMAND=pnpm test -- --runInBand
# Optional: critique-only command (used when AGENTATION_MODE=critique)
# AGENTATION_CRITIQUE_COMMAND=pnpm lint:tokens

CODEX_IMPLEMENTATION_TIMEOUT_MS=300000
CODEX_REVIEW_TIMEOUT_MS=180000
